{
    "name": "Dewall",
    "description": "Removes paywall from monitor.co.ug",
    "version": "0.0.0",
    "manifest_version": 3,
    "author": "Joseph Mawa",
    "action": {
        "default_popup": "public/popup.html",
        "default_title": "Remove paywall",
        "default_icon": {
            "16": "images/16.png",
            "24": "images/24.png",
            "32": "images/32.png"
        }
    },
    "background": {
        "service_worker": "src/background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "declarativeContent"
    ]
}